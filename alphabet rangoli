import string

a =string.ascii_lowercase
lis=[]
n=int(input())

for i in range(1,n):
    s='-'.join(a[:i])
    s1=list(s)
    s2=reversed(s1)
    s3=s1+(list(s2))[1:]
    s4='-'.join(s3)
    
    #s1='-'.join(a[::-1])
    #l1=l.append([::-1]+s[:1])
    
    
    print(s4.center(45))
    
for i in range(n,0,-1):
    s='-'.join(a[:i])
    s1=list(s)
    s2=reversed(s1)
    s3=s1+(list(s2))[1:]
    s4='-'.join(s3)
    print(s4.center(45))
