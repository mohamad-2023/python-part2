


def minion_game(s2):
    # your code goes here
    s1=str(s2)
    a='AEOUI'
    b=0
    f=0
    for i in range(len(s1)):
        if s1[i] not in a:
            b=b+len(s1[i:])
            #c+=b
    #for i in string:
        else:
            f+=len(s1[i:])
            #d+=f
    if b>f:
        print("Stuart",b) 
    elif b<f:
        print("Kevin",f)
    else:
        print("Draw")

if __name__ == '__main__':
    s = input()
    minion_game(s)
